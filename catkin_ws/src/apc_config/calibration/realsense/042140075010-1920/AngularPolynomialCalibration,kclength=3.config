aprilCameraCalibration {

    // MRE:   0.682 px MSE   0.737 px MaxRE   3.363 px

    names = [ camera0000 ];

    camera0000 {
        class = "april.camera.models.AngularPolynomialCalibration";

        width = 1920;
        height = 1080;

        intrinsics {
            // focal length fx, fy
            fc = [1370.433511,1369.442287 ];
            // focal center cx, cy
            cc = [ 951.077144, 550.239933 ];
            // polynomial approximation mapping theta to radius
            kc = [   0.532837,  -1.353587,   2.443459 ];
        }
        extrinsics {
            // Global-To-Camera coordinate transformation
            position = [   0.000000,   0.000000,   0.000000 ];
            rollpitchyaw_degrees = [   0.000000,  -0.000000,   0.000000 ];
        }
    }
}
